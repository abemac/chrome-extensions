{
	"manifest_version": 2,

	"name": "TypeSpeed",
	"description": "Tells you how fast you type",
	"version": "0.9",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background":{
		"page":"background.html",
		"persistent":true
	},
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["content.js"],
			"all_frames":true
		}
	],
	"permissions": [
		"activeTab",
		"tabs"
	]
}
